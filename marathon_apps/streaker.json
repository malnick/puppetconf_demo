{
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "malnick/streaker:1.0.0",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 8000, "hostPort": 0, "protocol": "tcp" }
      ]
    }
  },
  "id": "streaker-service",
  "instances": 1,
  "cpus": 0.5,
  "mem": 512,
  "uris": []
  "upgradeStrategy": {
    "minimumHealthCapacity": 0.9,
    "maximumOverCapacity": 0.5
  },
  "healthChecks": [
    {
      "protocol": "COMMAND",
      "command": { "value": "curl -f -X GET http://$HOST:$PORT0/" },
      "gracePeriodSeconds": 60,
      "intervalSeconds": 10,
      "timeoutSeconds": 2,
      "maxConsecutiveFailures": 6
    }
  ]
}
