{
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "malnick/microbrew",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 8000, "hostPort": 0, "protocol": "tcp" }
      ]
    }
  },
  "id": "microbrew-service",
  "instances": 3,
  "cpus": 0.5,
  "mem": 512,
  "upgradeStrategy": {
    "minimumHealthCapacity": 0.9,
    "maximumOverCapacity": 0.5
  }
}
